
<%- contentFor('body') %>
<h1>Lista de Produtos</h1>
<table>
    <thead>
        <tr>
            <th>ID</th>
            <th>Nome</th>
            <th>Pre√ßo</th>
        </tr>
    </thead>
    <tbody>
        <% produtos.forEach(produto => { %>
            <tr>
                <td><%= produto.id %></td>
                <td><%= produto.nome %></td>
                <td>R$ <%= produto.preco.toFixed(2) %></td>
            </tr>
        <% }); %>
    </tbody>
</table>